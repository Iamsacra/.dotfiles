alias kolourpaint='flatpak run org.kde.kolourpaint'
alias krita='flatpak run org.kde.krita'
alias keepassxc='flatpak run org.keepassxc.KeePassXC'
alias libreoffice='flatpak run org.libreoffice.LibreOffice'
